{
  "multipart": [
    {
      "apply": {
        "model": "morered:block/redstone_tube"
      },
      "when": {
        "powered": "false"
      }
    },
    {
      "apply": {
        "model": "morered:block/redstone_tube_on"
      },
      "when": {
        "powered": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true,
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true,
        "x": 90
      },
      "when": {
        "down": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "morered:block/tube_extension",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    }
  ]
}